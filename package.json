    "bluebird": "^3.7.1",
    "body-parser": "^1.19.0",
    "celebrate": "^10.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.17.0",
    "googleapis": "^45.0.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "sequelize": "^5.21.2",
