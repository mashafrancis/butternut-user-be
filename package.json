    "@types/cors": "^2.8.6",
    "@types/express-jwt": "^0.0.42",
    "@types/express-session": "^1.15.16",
    "@types/jest": "^24.0.23",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^12.12.11",
    "@types/nodemailer": "^6.2.2",
    "@types/sequelize": "^4.28.7",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^12.0.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
